@mixin borderRadius($size: 5px){
	border-radius:$size;
	-webkit-border-radius:$size;
	-moz-border-radius:$size;
	-ms-border-radius:$size;
	-o-border-radius:$size;
}

@mixin boxShadow($h){
	box-shadow: $h;
	-webkit-box-shadow: $h;
	-moz-box-shadow: $h;
	-ms-box-shadow: $h;
	-o-box-shadow: $h;
}

@mixin transform($t){
	transform:$t;
	-webkit-transform:$t;
	-moz-transform:$t;
	-ms-transform:$t;
	-o-transform:$t;
}

@mixin transition($t){
	transition:$t;
	-webkit-transition:$t;
	-moz-transition:$t;
	-ms-transition:$t;
	-o-transition:$t;
}

@mixin filter($f){
	-webkit-filter: $f;
	-moz-filter: $f;
	-o-filter: $f;
	-ms-filter: $f;
	filter: $f;
}



body {
	background-color:rgb(25,25,25);
	margin:0;
	font-family: Roboto,arial,sans-serif;
	overflow-x:hidden;
}

.pointer {
	cursor: pointer;
}

.floatRight {
	float:right;
}
.floatLeft {
	float:left;
}
.clearFix {
	clear:both;
}

.p_textLabel {
	position: relative;
	top: 45px;
	left: 15px;
	font-size:0.8em;
	font-weight:bold;
	float: left;
}

.p_textInput {
	box-sizing: border-box;
	padding: 20px 10px 20px 100px;
	font-size: 1.3em;
	width: 100%;
	letter-spacing: 2px;
}

.p_halfDiv {
	box-sizing: border-box;
	margin:0 0.5%;
	width:49%;
	float:left;
}

.p_halfDivNoBS {
	margin:0 0.5%;
	width:49%;
	float:left;
}

.p_button {
	-moz-box-shadow:inset 0px 1px 0px 0px #d9fbbe;
	-webkit-box-shadow:inset 0px 1px 0px 0px #d9fbbe;
	box-shadow:inset 0px 1px 0px 0px #d9fbbe;
	background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #b8e356), color-stop(1, #a5cc52) );
	background:-moz-linear-gradient( center top, #b8e356 5%, #a5cc52 100% );
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#b8e356', endColorstr='#a5cc52');
	background-color:#b8e356;
	-webkit-border-top-left-radius:10px;
	-moz-border-radius-topleft:10px;
	border-top-left-radius:10px;
	-webkit-border-top-right-radius:10px;
	-moz-border-radius-topright:10px;
	border-top-right-radius:10px;
	-webkit-border-bottom-right-radius:10px;
	-moz-border-radius-bottomright:10px;
	border-bottom-right-radius:10px;
	-webkit-border-bottom-left-radius:10px;
	-moz-border-radius-bottomleft:10px;
	border-bottom-left-radius:10px;
	text-indent:0;
	border:1px solid #83c41a;
	display:inline-block;
	color:#ffffff;
	font-family:Arial;
	font-size:15px;
	font-weight:bold;
	font-style:normal;
	height:52px;
	line-height:52px;
	min-width:100px;
	text-decoration:none;
	text-align:center;
	text-shadow:1px 1px 4px #86ae47;
	cursor:pointer;

	&:hover {
		background:-webkit-gradient( linear, left top, left bottom, color-stop(0.05, #a5cc52), color-stop(1, #b8e356) );
		background:-moz-linear-gradient( center top, #a5cc52 5%, #b8e356 100% );
		filter:progid:DXImageTransform.Microsoft.gradient(startColorstr='#a5cc52', endColorstr='#b8e356');
		background-color:#a5cc52;
	}
	&:active {
		position:relative;
		@include boxShadow( inset -1px 0px 10px rgb(66, 109, 20) );
	}
	&:focus {
		outline: none;
	}
}

.red {
	color:red;
}

.textOutline {
	color: white;
	text-shadow:-1px -1px 0 #000,  
				 1px -1px 0 #000,
				-1px  1px 0 #000,
				 1px  1px 0 #000;
}

.loadingCover {
	position:fixed;
	background-color:black;
	height:100%;
	width:100%;
	z-index:9999999;
}

.loadingCover p {
	position:absolute;
	left:50%;
	right:50%;
	top:50%;
	color:white;
	vertical-align:middle;
	width:20%;
}

.content {
	position:relative;
	box-sizing: border-box;
	top:100px;
	margin:0 auto;
	background-color:rgba(0,0,0,0.9);
	padding:50px 40px;
	border:1px solid black;
	border-radius:20px;
	text-align:left;
	width:80%;
	overflow:hidden;
}

.content:hover {
	overflow:auto;
}

/*************************

	Presentation.php
	
*************************/


/*************************

	Presentation - Main
	
*************************/
#selectModeWrapper {
	position:relative;
	text-align:center;
	
	h3 {
		 color:white;
	}
}

#createPresentationWrapper {
	display:block;
	margin:0 auto 50px auto;
	text-align:center;
	width:60%;
	
	h3 {
		text-align:left;
		color:white;
	}
}

#userPresentationsWrapper {
	position: relative;
	margin: 40px auto;
	color: white;
	width: 70%;
	clear:both;
}

.userPresentationPicker {
	display:block;
	box-sizing:border-box;
	background-color:rgb(60, 60, 60);
	padding:10px 20px;
	border:1px solid gray;
	width:100%;
	float:left;
	clear:both;
	
	p {
		font-size: 1.4em;
		color:white;
		float:left;
		
		a {
			color:white;
			text-decoration:none;
		}
	}
	
	.userPresentationOptions {
		font-size:0.6em;
	}
}

/*************************

	Presentation - Show
	
*************************/

/*************************

	Presentation - Edit
	
*************************/
.newSlideWrapper {
	position: absolute;
	top: 0px;
	background-color:rgba(0,0,0,0.8);
	z-index: 999999999999;
	width: 100%;
	height: 100%;

	.newSlideInner {
		position:relative;
		top: 7%;
		box-sizing:border-box;
		margin:10px auto;
		text-align:center;
		width:60%;
		
		h3 {
			color:white;
		}
		
		.newSlideTitle {
			box-sizing:border-box;
			padding:10px 20px;
			text-align:left;
			width:100%
		}
		
		.newSlideTextarea {
			position:relative;
			box-sizing:border-box;
			font-size:1em;
			width:80%;
			height:300px;
			float:left;
			resize:vertical;
		}
		
		.editPluginOptionsTextarea {
			width:100%;
		}
		
		.newSlideTextareaLabel {
			position: absolute;
			top: 150px;
			z-index: 9;
			
			&:nth-of-type(3){
				left: 81%;
			}
		}
		
		.remoteNoteEdit {
			position:relative;
			box-sizing:border-box;
			padding: 50px 10px;
			font-size:1em;
			width:20%;
			height:300px;
			float:left;
			resize:vertical;
		}
		
		#pluginOptionsSimple {
			background-color:white;
			margin-bottom:10px;
			padding:40px;
			border:2px solid darkgray;
			text-align:left;
			
			div {
				position: relative;
				margin: 0 auto;
				width: 80%;
			}
			label {
				display:inline-block;
				position:relative;
				text-align:right;
				width:25%;
			}
			select {
				background-color: white;
				border: 1px solid lightgray;
				padding: 10px 0px 10px 15px;
				width:25%;
				
				&:focus {
					outline: 1px solid black;
				}
			}
			input[type='color'] {
				width:25%;
			}
		}
		
	}
	
	#slideBackgroundHelper {
		top: 0px;
		padding: 0 20px;		
		background-color:white;
		border:1px solid black;
	}
}

.editSlide {
	position:absolute;
	top:15px;
	right:30px;
	z-index:99999999999;
}

.editSlide:active {
	position:absolute !important;
	top: 15px;
}

.removeSlide {
	position:absolute;
	top:15px;
	right:140px;
	z-index:99999999999;
}

.removeSlide:active {
	position:absolute !important;
	top: 15px;
}

.slideEditorButtons {
	background-color:rgb(116, 150, 57);
	padding:10px 5px;
	border-top:1px solid black;
	clear:both;
}

.bgORco {
	position: relative;
	top: 20px;
	margin: 0 10px;
	color: white;
	}

/****************************************************************************

						REMOTE
						
****************************************************************************/
					
#connect {
	float:right;
}

.remote-pairCode {
	padding:10px;
	width:30%;
}

.remote-arrowControls {
	width:50%;
	float:left;
	
	.controls {
		margin:20px 0;
		text-align:center;
		width:100%;
		height:200px;
		img {
			width:100%;
		}
	}
	#nextSlide {
		background:url('../../img/icon/next-filledWhite.png') no-repeat 50% / 100% 100%;
		
		&:active {
			background:url('../../img/icon/next-filledBlack.png') no-repeat 50% / 100% 100%;
		}
	}
	#previousSlide {
		background:url('../../img/icon/previous-filledWhite.png') no-repeat 50% / 100% 100%;
		
		&:active {
			background:url('../../img/icon/previous-filledBlack.png') no-repeat 50% / 100% 100%;
		}
	}

	.controls:active {
		
	}
}

.remote-presentationSlide {
	display:block;
	box-sizing:border-box;
	background-size:100% 100% !important;
	background-repeat:no-repeat;
	margin:5px;
	border:1px solid white;
	width: 10%;
	min-width:100px;
	height:80px;
	float:left;
}

.remote-slideActive {
	border:7px solid green;
}

.remoteNotesWrapper {
	display: block;
	position:relative;
	left:50%;
	padding:10px;
	color: white;
	clear: both;
	
	h4 {
		margin:0;
	}
	
	hr {
		margin-top:1px;
		margin-bottom:4px;
		width:100%;
	}
}
.remoteNotes {
	padding-left:10px;
}

#exitRemoteMode {
	position:absolute;
	right:5px;
	top:3px;
}


@media screen and (max-width: 800px) {
	.userPresentationOptions{
		display:block;
		clear:both;
	}
	
	.remote-arrowControls {
		width:100%;

		.controls {
			margin:20px 0;
			text-align:center;
			width:100%;
			height:200px;
			img {
				width:100%;
			}
		}
		#nextSlide {
			background:url('../../img/icon/next-filledWhite.png') no-repeat 50% / 100% 100%;

			&:active {
				background:url('../../img/icon/next-filledBlack.png') no-repeat 50% / 100% 100%;
			}
		}
		#previousSlide {
			background:url('../../img/icon/previous-filledWhite.png') no-repeat 50% / 100% 100%;

			&:active {
				background:url('../../img/icon/prev-filledBlack.png') no-repeat 50% / 100% 100%;
			}
		}
	}
	.remoteNotesWrapper {
		left:0;
	}

}

@media screen and (max-width: 600px) {
	.p_textLabel {
		position: relative;
		top: 45px;
		left: 15px;
		font-size:0.8em;
		font-weight:bold;
		float: left;
	}

	.p_textInput {
		box-sizing: border-box;
		padding: 20px 10px 20px 100px;
		font-size: 1em !important;
		width: 100%;
		letter-spacing: 2px;
	}

	.p_halfDiv {
		box-sizing: border-box;
		margin:0 0.5%;
		width:100%;
	}

	.p_halfDivNoBS {
		margin:0 0.5%;
		width:100%;
	}
	
	.remote-pairCode {
		padding: 10px;
		width: 100%;
	}
	.pairMe {
		float:right
	}
	
	#signInForm {
		width:98% !important;
		
		div {
			width:100%
		}
	}
	.remoteNotesWrapper {
		left:0;
	}
}



/****************************************************************************

							MISC

****************************************************************************/
.slimScrollDiv {
	float:left;
}
.slimScrollBar {
	z-index:999999999 !important;
}
